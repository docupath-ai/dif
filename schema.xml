<?xml version="1.0" encoding="UTF-8"?>
<Invoice
	xmlns="https://www.docupath.ai/specifications/DIF"
	xmlns:cac="urn:docupath:CommonAggregateComponents:1.0"
	xmlns:cbc="urn:docupath:CommonBasicComponents:1.0">
	<cbc:InvoiceNumber>{invoice_number}</cbc:InvoiceNumber>
	<cbc:IssueDate>{invoice_date}</cbc:IssueDate>
	<cbc:DueDate>{invoice_due_date}</cbc:DueDate>
	<cbc:DocumentCurrencyCode>{invoice_currency_code}</cbc:DocumentCurrencyCode>
	<cac:AccountingCustomerParty>
		<cac:Party>
			<cbc:Name>{buyer_name}</cbc:Name>
			<cbc:ElectronicMail>{buyer_email}</cbc:ElectronicMail>
			<cac:PostalAddress>
				<cbc:StreetName>{buyer_address}</cbc:StreetName>
				<cbc:CityName>{buyer_city}</cbc:CityName>
				<cbc:PostalZone>{buyer_postal_code}</cbc:PostalZone>
				<cac:Country>
					<cbc:IdentificationCode>{buyer_country_code}</cbc:IdentificationCode>
					<cbc:Name>{buyer_country}</cbc:Name>
				</cac:Country>
			</cac:PostalAddress>
			<cac:PartyIdentification>
				<cbc:ID schemeID="GLN">{gln_no}</cbc:ID>
				<cbc:ID schemeID="PageroID">{pagero_id}</cbc:ID>
				<cbc:CompanyID schemeID="VAT">{buyer_vat_no}</cbc:CompanyID>
				<cbc:CompanyID schemeID="RegistrationNo">{registration_no}</cbc:CompanyID>
			</cac:PartyIdentification>
		</cac:Party>
	</cac:AccountingCustomerParty>
	<cac:AccountingSupplierParty>
		<cbc:PostingDate>{invoice_posting_date}</cbc:PostingDate>
		<cac:Party>
			<cbc:Name>{seller_name}</cbc:Name>
			<cac:PostalAddress>
				<cbc:StreetName>{seller_address}</cbc:StreetName>
				<cbc:CityName>{seller_city}</cbc:CityName>
				<cbc:PostalZone>{seller_postal_code}</cbc:PostalZone>
				<cac:Country>
					<cbc:IdentificationCode>{seller_country_code}</cbc:IdentificationCode>
					<cbc:Name>{seller_country}</cbc:Name>
				</cac:Country>
			</cac:PostalAddress>
			<cac:PartyIdentification>
				<cbc:CompanyID schemeID="VAT">{seller_vat_no}</cbc:CompanyID>
			</cac:PartyIdentification>
		</cac:Party>
	</cac:AccountingSupplierParty>
	<cac:PayeeParty>
		<cac:Party>
			<cbc:Name>{remit_to_name}</cbc:Name>
			<cac:PostalAddress>
				<cbc:StreetName>{remit_to_street}</cbc:StreetName>
				<cbc:AdditionalStreetName>{remit_to_street2}</cbc:AdditionalStreetName>
				<cbc:CityName>{remit_to_city}</cbc:CityName>
				<cbc:CountrySubentity>{remit_to_province}</cbc:CountrySubentity>
				<cbc:PostalZone>{remit_to_postal_code}</cbc:PostalZone>
				<cac:Country>
					<cbc:IdentificationCode>{remit_to_country_code}</cbc:IdentificationCode>
					<cbc:Name>{remit_to_country}</cbc:Name>
				</cac:Country>
			</cac:PostalAddress>
		</cac:Party>
	</cac:PayeeParty>
	<cac:Delivery>
		<cac:DeliveryLocation>
			<cbc:Name>{shipping_to_name}</cbc:Name>
			<cac:PostalAddress>
				<cbc:StreetName>{shipping_address}</cbc:StreetName>
				<cbc:CityName>{shipping_city}</cbc:CityName>
				<cbc:PostalZone>{shipping_postal_code}</cbc:PostalZone>
				<cac:Country>
					<cbc:IdentificationCode>{shipping_country_code}</cbc:IdentificationCode>
					<cbc:Name>{shipping_country}</cbc:Name>
				</cac:Country>
			</cac:PostalAddress>
		</cac:DeliveryLocation>
		<cac:DeliveryTerms>
			<cbc:ID>{invoice_delivery_note_no}</cbc:ID>
			<cbc:SpecialTerms>{invoice_delivery_date}</cbc:SpecialTerms>
		</cac:DeliveryTerms>
		<cbc:DeliveryMethod>{shipping_method}</cbc:DeliveryMethod>
		<cbc:TrackingID>{shipping_tracking_number}</cbc:TrackingID>
	</cac:Delivery>
	<cac:PaymentTerms>
		<cbc:Description>{invoice_payment_terms}</cbc:Description>
		<cbc:PaymentMeansCode>{payment_reference}</cbc:PaymentMeansCode>
		<cbc:ID>{payment_uuid}</cbc:ID>
		<cbc:PayableAmount>{payment_amount_due}</cbc:PayableAmount>
		<cbc:Note>{payment_notes}</cbc:Note>
	</cac:PaymentTerms>
	<cac:OrderReference>
		<cbc:ID>{invoice_ref_cost_center}</cbc:ID>
		<cbc:SalesOrderID>{invoice_purchase_order_no}</cbc:SalesOrderID>
	</cac:OrderReference>
	<cac:Items>
		<!--ITEM START-->
		<cac:Item>
			<cbc:Description>{item_description}</cbc:Description>
			<cbc:Name>{item_article_name}</cbc:Name>
			<cac:SellersItemIdentification>
				<cbc:ID>{item_article_id}</cbc:ID>
			</cac:SellersItemIdentification>
			<cac:BuyersItemIdentification>
				<cbc:ID>{item_buyer_article_id}</cbc:ID>
			</cac:BuyersItemIdentification>
			<cac:StandardItemIdentification>
				<cbc:ID>{item_note_number}</cbc:ID>
			</cac:StandardItemIdentification>
			<cac:ClassifiedTaxCategory>
				<cbc:Percent>{item_tax_rate}</cbc:Percent>
				<cac:TaxScheme>
					<cbc:ID>{item_tax_amount}</cbc:ID>
				</cac:TaxScheme>
			</cac:ClassifiedTaxCategory>
			<cac:ItemInstance>
				<cbc:ProductTraceID>{item_your_reference_number}</cbc:ProductTraceID>
			</cac:ItemInstance>
			<cac:Price>
				<cbc:PriceAmount>{item_unit_price}</cbc:PriceAmount>
				<cbc:BaseQuantity unitCode="{item_uom}">{item_quantity}</cbc:BaseQuantity>
			</cac:Price>
			<cac:AllowanceCharge>
				<cbc:Amount>{item_discount}</cbc:Amount>
			</cac:AllowanceCharge>
			<cac:LineItem>
				<cbc:ID>{item_order_number}</cbc:ID>
				<cbc:LineExtensionAmount>{item_net_amount}</cbc:LineExtensionAmount>
				<cbc:TotalTaxAmount>{item_tax_amount}</cbc:TotalTaxAmount>
				<cbc:GrossPriceAmount>{item_gross_amount}</cbc:GrossPriceAmount>
			</cac:LineItem>
		</cac:Item>
		<!--ITEM END-->
	</cac:Items>
	<!--PAYMENT METHODS START-->
	<cac:PaymentMeans>
		<cbc:PaymentMeansCode>{payment_method}</cbc:PaymentMeansCode>
		<cbc:InstructionNote>{payment_method_description}</cbc:InstructionNote>
	</cac:PaymentMeans>
	<!--PAYMENT METHODS END-->
	<cac:TaxTotal>
		<cbc:TaxAmount>{invoice_total_tax_amount}</cbc:TaxAmount>
		<!--TAX START-->
		<cac:TaxSubtotal>
			<cbc:TaxAmount>{tax_amount}</cbc:TaxAmount>
			<cac:TaxCategory>
				<cbc:ID>{tax_code}</cbc:ID>
				<cbc:Percent>{tax_rate}</cbc:Percent>
				<cac:TaxExchangeRate>
					<cbc:CalculationRate>{tax_exchange_rate}</cbc:CalculationRate>
				</cac:TaxExchangeRate>
			</cac:TaxCategory>
			<cbc:BaseUnitMeasure>{tax_amount_diff}</cbc:BaseUnitMeasure>
		</cac:TaxSubtotal>
		<!--TAX END-->
	</cac:TaxTotal>
	<cac:UserPaymentInfo>
		<cbc:PaymentSubTotal>{payment_sub_total}</cbc:PaymentSubTotal>
		<cbc:PaymentTotal>{payment_total}</cbc:PaymentTotal>
		<cbc:PaymentDueDate>{payment_amount_due}</cbc:PaymentDueDate>
	</cac:UserPaymentInfo>
	<cac:LegalMonetaryTotal>
		<cbc:LineExtensionAmount>{invoice_net_amount}</cbc:LineExtensionAmount>
		<cbc:PayableRoundingAmount>{invoice_rounding_amount}</cbc:PayableRoundingAmount>
		<cbc:PayableAmount>{invoice_total_amount}</cbc:PayableAmount>
	</cac:LegalMonetaryTotal>
</Invoice>
